<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<?scenebuilder-stylesheet drawer.css?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="200" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
    
      
    <!--image init -->
   
     <ImageView fitHeight="200" fitWidth="200" layoutX="5" layoutY="5" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="file:APPFiles/images/placeholder.JPG" />
         </image>
     </ImageView>
		
    <!--image ends -->
		
      <JFXButton fx:id="acount" accessibleText="account" prefHeight="50" prefWidth="200" style="-fx-text-fill:#0f33e2;-fx-font-size:16px;" text="My Account" >
             <stylesheets>
                <URL value="@/drawer.css" />
            </stylesheets> 
       </JFXButton>
          
      <JFXButton fx:id="support" accessibleText="support" prefHeight="50" prefWidth="200" style="-fx-text-fill:#0f33e2;-fx-font-size:16px;" text="Support" >
               
       </JFXButton>
      <JFXButton fx:id="about" accessibleText="about" prefHeight="50" prefWidth="200" style="-fx-text-fill:#0f33e2;-fx-font-size:16px;" text="About us" >
             
       </JFXButton>
      <JFXButton fx:id="exit" accessibleText="exit" buttonType="RAISED" prefHeight="50" prefWidth="200" style="-fx-text-fill:#0f33e2;-fx-font-size:16px;" text="Exit" >
                  
